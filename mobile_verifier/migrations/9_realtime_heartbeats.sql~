DROP TABLE heartbeats;

CREATE TYPE cell_type AS ENUM (
       'nova436h',
       'nova430i',
       'neutrino430',
       'sercommindoor',
       'sercommoutdoor'
);

CREATE TABLE heartbeats (
       cbsd_id TEXT NOT NULL PRIMARY KEY,
       hotspot_key TEXT NOT NULL,
       truncated_timestamp TIMESTAMP NOT NULL
);
